{
  "name": "qantas-code-test",
  "version": "1.0.0",
  "description": "A React boilerplate with babel, eslint, prettier and etc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuchenQ/Qantas-Code-Test.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "start": "NODE_ENV=development webpack-dev-server --config webpack/webpack.config.dev.js",
    "lint": "concurrently npm:lint:*",
    "lint:js": "eslint './src/**/*.{jsx,js}' --quiet --fix",
    "lint:styles": "stylelint './src/**/*.jsx'",
    "test": "concurrently \"npm:test:unit\" \"npm:test:snap\"",
    "test:unit": "jest src --config ./jest.config.json --coverage --coverageReporters=text --passWithNoTests",
    "test:snap": "jest storyshots --config ./jest.config.json",
    "test:snap:update": "npm run test:snap -- -u",
    "storybook": "start-storybook"
  },
  "author": "Yuchen Qiao",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick",
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "pretty-quick",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.8.4",
    "concurrently": "^5.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.9.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-backgrounds": "^5.3.10",
    "@storybook/addon-knobs": "^5.3.10",
    "@storybook/addon-notes": "^5.3.10",
    "@storybook/addon-storyshots": "^5.3.10",
    "@storybook/addon-viewport": "^5.3.10",
    "@storybook/addons": "^5.3.10",
    "@storybook/react": "^5.3.10",
    "@testing-library/jest-dom": "^5.1.0",
    "@testing-library/react": "^9.4.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jest-dom": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.2",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.1.0",
    "jest-styled-components": "^7.0.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^10.0.7",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "sinon": "^8.1.1",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "terser-webpack-plugin": "^2.3.4",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.10.2",
    "webpack-merge": "^4.2.2"
  }
}
